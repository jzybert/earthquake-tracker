<div class="row">
  <div class="col-12">
    <h1>Welcome to Earthquake Tracker</h1>
    <%= if !@current_user do %>
      <%= link "Click here to register", to: Routes.user_path(@conn, :new) %>
    <% else %>
        <%= render EarthquakeTrackerWeb.EarthquakeQueryView, "index.html", conn: @conn %>
    <% end %>
  </div>
</div>
